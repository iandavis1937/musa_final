	file = {Full Text PDF:C\:\\Users\\ianda\\Zotero\\storage\\QI4KUBHS\\Yamazaki and Nagao - 2012 - A Computational Mechanism for Unified Gain and Tim.pdf:application/pdf;Snapshot:C\:\\Users\\ianda\\Zotero\\storage\\W5IK56BY\\article.html:text/html},
