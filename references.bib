	journal = {PLOS ONE},
	author = {Yamazaki, Tadashi and Nagao, Soichi},
	month = mar,
	year = {2012},
	note = {Publisher: Public Library of Science},
	keywords = {Action potentials, Neurons, Network analysis, Cerebellum, Golgi cells, Granule cells, Purkinje cells, Synapses},
