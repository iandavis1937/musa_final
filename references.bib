	pages = {135--158},
	file = {Accepted Version:C\:\\Users\\ianda\\Zotero\\storage\\9T3NJFN5\\Chen and Miller - 2012 - “Shift-and-Persist” Strategies Why Low Socioecono.pdf:application/pdf},
}

