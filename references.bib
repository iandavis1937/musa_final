@article{chen_shift-and-persist_2012,
	title = {“{Shift}-and-{Persist}” {Strategies}: {Why} {Low} {Socioeconomic} {Status} {Isn}’t {Always} {Bad} for {Health}},
	volume = {7},
	issn = {1745-6916, 1745-6924},
